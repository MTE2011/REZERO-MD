# Bot Token
TOKEN=your_bot_token_here

# Bot Prefix
PREFIX=.

# Main owner of the bot
OWNER_ID=your_discord_id_here

# Channel for ticket messages
TICKET_CHANNEL_ID=123456789012345678

# Channel for partnership tickets (ps.js)
PARTNERSHIP_CHANNEL_ID=123456789012345678

# Channel for private partnership tickets (pps.js)
PRIVATE_PARTNERSHIP_CHANNEL_ID=123456789012345678

# Channel for news messages (news.js)
NEWS_CHANNEL_ID=123456789012345678

# Channel for connected/ready messages
LOG_CHANNEL_ID=123456789012345678

# Role to ping for partnerships
PARTNER_ROLE_ID=123456789012345678

# Staff role for tickets
STAFF_ROLE_ID=123456789012345678

# Category ID where tickets will be created
TICKET_CATEGORY_ID=123456789012345678

# Channel for ticket logs
TICKET_LOG_CHANNEL_ID=123456789012345678

# Guards (Moderators who can access tickets and use moderation commands)
# Separate multiple IDs with commas
GUARDS_ID=id1,id2,id3
